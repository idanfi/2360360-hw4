// simple switch
void main() {
   int i = 0;
   while (i < 3) {
      switch (i) {
          case 1: {
             printi(i);
             break;
          }
          case 2: {
             printi(i);
             break;
          }
          default: {
             printi(i);
          }
      }
      i = i + 1;
   }
}