// simple switch
void main() {
   int i = 0;
   while (i < 3) {
      switch (i) {
          case 1: {
             print("case 1");
             break;
          }
          case 2: {
             print("case 2");
          }
          default: {
             print("default");
          }
      }
      i = i + 1;
   }
}